              },
              "@media (max-width: 767px)": {
                "padding": "14px 20px",
                "border": "1px solid darken(@SliderBg, 5.8%)",
                "&:not(.last-child)": {
                  "border-bottom": "none"
                }
              },
              "color": "#485969"
            },
            ".tabButton[selected]": {
              "color": "@primaryColor",
              "background-color": "@SliderBg",
              "outline": "none",
              "@media (max-width: 767px)": {
                "background-color": "transparent",
                "&:not(.last-child)": {
                  "border-bottom": "1px solid darken(@SliderBg, 5.8%)"
                }
              }
            },
            ".tabButton[selected] + .tabContent": {
              "display": "block"

        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "overflow": "visible",
            "padding": "0",
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0"
              }
            },
            ".navbar": {
              "background-color": "@menuBgColor"
            },
            ".menu__block, .contact__block": {
              "width": "100%",
              "padding": "0 1.875rem"
            },
            ".navbar-logo when (@showLogo)": {
              "amp-img,img": {
                "object-fit": "contain",
                "height": "~\"@{logoHeight}px\" !important",
                "width": "~\"@{logoWidth}px\" !important"
              }
            },
            ".navbar .navbar-collapse": {
              "@media (max-width: 991px)": {
                "background": "@menuBgColor",
                "& when (@sidebarSide = \"left\")": {
                  "right": "auto",
                  "left": "0"
                }
              }
            },
            "& when (@showItems)": {
              ".nav-link": {
                "padding": "1.875rem 1rem 1.625rem !important",
                "margin": "0 !important",
                "position": "relative"
              },
              ".dropdown-item.active, .dropdown-item:active": {
                "background-color": "transparent"
              },
              ".dropdown-menu": {
                "background": "lighten(@menuBgColor, 10%)",
                "z-index": "11",
                "padding": "1.06rem 1.875rem",
                "box-shadow": "0 0 35px 0 rgba(0, 0, 0, 0.07)",
                "border-radius": "4px"
              },
              ".dropdown-item": {
                "border-bottom": "1px solid darken(contrast(@menuBgColor), 20%)",
                "&:last-child": {
                  "border-bottom": "none"
                }
              },
              ".nav-item": {
                "position": "relative"
              },
              ".nav-item .nav-link::before": {
                "content": "''",
                "display": "block",
                "pointer-events": "none",
                "position": "absolute",
                "bottom": "0",
                "left": "0",
                "z-index": "1",
                "height": "3px",
                "width": "100%",
                "transition": "all 0.2s ease"
              },
              ".nav-item:hover .nav-link::before": {
                "background-color": "@primaryColor"
              }
            },
            ".hamburger span": {
              "background-color": "@hamburgerColor"
            },
            ".builder-sidebar": {
              "background-color": "@menuBgColor"
            },
            ".mbr-align-center": {
              "align-items": "center"
            },
            ".mbr-jc-sb when (@showLogo), (@showBrand), (@showQuest), (@showSocial)": {
              "justify-content": "space-between"
            },
            "& when (@showSocial)": {
              ".amp-iconfont": {
                "font-size": "14px",
                "width": "14px",
                "color": "contrast(@menuBgColor) !important",
                "opacity": ".5",
                "&:hover": {
                  "opacity": "1"
                }
              }
            },
            "& when (@showContact)": {
              "@media (min-width: 992px)": {
                "&.menu": {
                  "min-height": "118px"
                }
              },
              ".contact__block": {
                "height": "35px"
              },
              ".border__bottom": {
                "border-bottom": "1px solid fade(contrast(@menuBgColor), 20%)",
                "width": "100%"
              },
              ".contact__text": {
                "margin-right": "20px",
                "transition": "all 0.2s ease",
                "&:hover": {
                  "color": "@primaryColor"
                }
              },
              ".mbr-iconfont": {
                "margin-right": "8px",
                "width": "18px",
                "font-size": "18px",
                "transition-duration": "0s"
              }
            },
            "& when (@showQuest)": {
              ".questions:hover": {
                ".phone": {
                  "color": "@primaryColor",
                  "transition": "color .3s ease"
                }
              },
              ".quest__icon": {
                "margin-right": "20px",
                ".mbr-iconfont": {
                  "font-size": "35px",
                  "width": "35px",
                  "height": "40px",
                  "color": "@primaryColor"
                }
              }
            },
            "@media (max-width: 991px)": {
              ".contact__block, .border__bottom, .menu-social-list, .questions": {
                "display": "none"
              },
              ".dropdown-menu": {
                "margin": "0"
              }
            },
            ".adress": {
              "text-align": "right"
            }

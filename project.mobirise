        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "overflow": "visible",
            "padding": "0",
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0"
              }
            },
            ".navbar": {
              "background-color": "@menuBgColor"
            },
            ".menu__block, .contact__block": {
              "width": "100%",
              "padding": "0 1.875rem"
            },
            ".navbar-logo when (@showLogo)": {
              "amp-img,img": {
                "object-fit": "contain",
                "height": "~\"@{logoHeight}px\" !important",
                "width": "~\"@{logoWidth}px\" !important"
              }
            },
            ".navbar .navbar-collapse": {
              "@media (max-width: 991px)": {
                "background": "@menuBgColor",
                "& when (@sidebarSide = \"left\")": {
                  "right": "auto",
                  "left": "0"
                }
              }
            },
            "& when (@showItems)": {
              ".nav-link": {
                "padding": "1.875rem 1rem 1.625rem !important",
                "margin": "0 !important",
                "position": "relative"
              },
              ".dropdown-item.active, .dropdown-item:active": {
                "background-color": "transparent"
              },
              ".dropdown-menu": {
                "background": "lighten(@menuBgColor, 10%)",
                "z-index": "11",
                "padding": "1.06rem 1.875rem",
                "box-shadow": "0 0 35px 0 rgba(0, 0, 0, 0.07)",
                "border-radius": "4px"
              },
              ".dropdown-item": {
                "border-bottom": "1px solid darken(contrast(@menuBgColor), 20%)",
                "&:last-child": {
                  "border-bottom": "none"
                }
              },
              ".nav-item": {
                "position": "relative"
              },
              ".nav-item .nav-link::before": {
                "content": "''",
                "display": "block",
                "pointer-events": "none",
                "position": "absolute",
                "bottom": "0",
                "left": "0",
                "z-index": "1",
                "height": "3px",
                "width": "100%",
                "transition": "all 0.2s ease"
              },
              ".nav-item:hover .nav-link::before": {
                "background-color": "@primaryColor"
              }
            },
            ".hamburger span": {
              "background-color": "@hamburgerColor"
            },
            ".builder-sidebar": {
              "background-color": "@menuBgColor"
            },
            ".mbr-align-center": {
              "align-items": "center"
            },
            ".mbr-jc-sb when (@showLogo), (@showBrand), (@showQuest), (@showSocial)": {
              "justify-content": "space-between"
            },
            "& when (@showSocial)": {
              ".amp-iconfont": {
                "font-size": "14px",
                "width": "14px",
                "color": "contrast(@menuBgColor) !important",
                "opacity": ".5",
                "&:hover": {
                  "opacity": "1"
                }
              }
            },
            "& when (@showContact)": {
              "@media (min-width: 992px)": {
                "&.menu": {
                  "min-height": "118px"
                }
              },
              ".contact__block": {
                "height": "35px"
              },
              ".border__bottom": {
                "border-bottom": "1px solid fade(contrast(@menuBgColor), 20%)",
                "width": "100%"
              },
              ".contact__text": {
                "margin-right": "20px",
                "transition": "all 0.2s ease",
                "&:hover": {
                  "color": "@primaryColor"
                }
              },
              ".mbr-iconfont": {
                "margin-right": "8px",
                "width": "18px",
                "font-size": "18px",
                "transition-duration": "0s"
              }
            },
            "& when (@showQuest)": {
              ".questions:hover": {
                ".phone": {
                  "color": "@primaryColor",
                  "transition": "color .3s ease"
                }
              },
              ".quest__icon": {
                "margin-right": "20px",
                ".mbr-iconfont": {
                  "font-size": "35px",
                  "width": "35px",
                  "height": "40px",
                  "color": "@primaryColor"
                }
              }
            },
            "@media (max-width: 991px)": {
              ".contact__block, .border__bottom, .menu-social-list, .questions": {
                "display": "none"
              },
              ".dropdown-menu": {
                "margin": "0"
              }
            },
            ".adress": {
              "text-align": "right"
            }
          },
          "_name": "ampMenu7",
          "_customHTML": "<section class=\"ampMenu7 menu horizontal-menu\" group=\"Menu\" plugins=\"DropDown, amp-main, amp-sidebar\" always-top global once=\"menu\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <!--  -->\n        <header>Position</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n        <!--  -->\n        <header>Show/Hide Contacts</header>\n        <input type=\"checkbox\" title=\"Show Contacts\" name=\"showContact\">\n        <input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\" condition=\"showContact\" checked>\n        <input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"1\" max=\"6\" value=\"4\" step=\"1\" condition=\"showSocial && showContact\">\n        <select name=\"alignContact\" title=\"Align Contact\" condition=\"!showSocial\">\n            <option value=\"1\">left</option>\n            <option value=\"2\">center</option>\n            <option value=\"3\" selected>right</option>\n        </select>\n        <header>Show/Hide Menu</header>\n        <input type=\"checkbox\" title=\"Show Logo\" name=\"showLogo\">\n        <input type=\"range\" title=\"Logo Height\" inline name=\"logoHeight\" min=\"30\" max=\"60\" value=\"60\" step=\"2\" condition=\"showLogo\">\n        <input type=\"range\" title=\"Logo Width\" inline name=\"logoWidth\" min=\"70\" max=\"160\" value=\"150\" step=\"2\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Show Menu Items\" name=\"showItems\" checked>\n       <input type=\"checkbox\" title=\"Show Questions\" name=\"showQuest\" checked>\n        <select title=\"Sidebar\" name=\"sidebarSide\">\n            <option value=\"left\">Left</option>\n            <option value=\"right\" selected>Right</option>\n        </select>\n        <input type=\"color\" title=\"Hamburger Color\" name=\"hamburgerColor\" value=\"#ffffff\">\n        <header>Background</header>\n        <input type=\"color\" title=\"Color\" name=\"menuBgColor\" value=\"#185886\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <!-- <div class=\"menu-wrapper\"> -->\n   \n    <nav class=\"navbar navbar-dropdown navbar-expand-lg\" mbr-class=\"{'navbar-fixed-top': sticky}\">\n        <div class=\"menu-container mbr-column mbr-jc-c\" mbr-class=\"{'container': !fullWidth,\n                        'container-fluid': fullWidth}\">\n            <div class=\"mbr-flex contact__block mbr-align-center\" mbr-if=\"showContact\" mbr-class=\"{'mbr-jc-s' : !showSocial && alignContact == 1, 'mbr-jc-c' : !showSocial && alignContact == 2, 'mbr-jc-e' : !showSocial && alignContact == 3, 'mbr-jc-sb' : showSocial}\">\n                <div class=\"mbr-flex mbr-white\">\n                   <a href=\"#\"><p class=\"contact__text mbr-fonts-style mbr-white\" mbr-theme-style=\"display-4\" data-app-selector=\".contact__text\"><span class=\"fa fa-envelope mbr-iconfont\"></span>company@mail.com</p></a>\n                    <a href=\"#\"><p class=\"contact__text mbr-fonts-style mbr-white\" mbr-theme-style=\"display-4\" data-app-selector=\".contact__text\"><span class=\"fa fa-envelope mbr-iconfont\"></span>company@mail.com</p></a>\n                    <p class=\"mbr-fonts-style adress\" mbr-theme-style=\"display-4\" data-app-selector=\".adress\"><span class=\"mbrib-pin mbr-iconfont\"></span>585 Boulevard Charest, Canada</p>\n                </div>\n                 <!-- SOCIAL ICON -->\n                <div class=\"menu-social-list\" mbr-if=\"showSocial\" mbr-list mbr-list-grow=\"iconsCount\" mbr-link-type=\"socials\">\n                    <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-youtube-play fa\"></span>\n                    </a>\n                    <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-facebook-f fa\"></span>\n                    </a>\n                    <a href=\"https://plus.google.com/u/0/+Mobirise/posts\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-linkedin fa\"></span>\n                    </a>\n                <a href=\"https://plus.google.com/u/0/+Mobirise/posts\" target=\"_blank\">\n                        <span mbr-icon class=\"amp-iconfont fa-vimeo-square fa\"></span>\n                    </a></div>\n                <!-- SOCIAL ICON END -->\n            </div>\n            <div class=\"border__bottom\" mbr-if=\"showContact\"></div>\n    <!-- SHOW LOGO -->\n            <div class=\"mbr-flex menu__block\" mbr-class=\"{'mbr-jc-sb': showLogo || showBrand || showQuest, 'mbr-jc-c' : ((showItems) && (!showLogo || !showBrand || !showQuest))}\">\n            <div class=\"navbar-brand\" mbr-if=\"showLogo || showBrand\">\n                <div mbr-if=\"showLogo\" class=\"navbar-logo\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/logo-1.png\" layout=\"responsive\" width=\"50\" height=\"50\" alt=\"Mobirise\" class=\"mobirise-loader\">\n                        <div placeholder class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/logo-1.png\" alt=\"Mobirise\">\n                    </amp-img>\n                </div>\n                <span mbr-if=\"showBrand\" mbr-buttons mbr-theme-style=\"display-7\" class=\"navbar-caption-wrap\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\"><a class=\"navbar-caption mbr-semibold text-white text-primary\" data-app-selector=\".navbar-caption\" href=\"index.html\" data-app-placeholder=\"Type Text\">RESOLUTION BAZAAR</a></span>\n            </div>\n            <!-- SHOW LOGO END -->\n            <!-- COLLAPSED MENU -->\n            <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n                <button class=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-controls=\"navbarNavAltMarkup\" aria-expanded=\"false\" aria-label=\"Toggle navigation\" mbr-if=\"showItems || showSocial\">\n                    <div class=\"hamburger\">\n                        <span></span>\n                        <span></span>\n                        <span></span>\n                        <span></span>\n                    </div>\n                </button>\n                <!-- NAVBAR ITEMS -->\n                <ul mbr-menu class=\"navbar-nav nav-dropdown\" mbr-theme-style=\"display-4\" mbr-if=\"showItems\" mbr-class=\"{'navbar-nav-top-padding': isPublish && !showBrand && !showLogo}\"><li class=\"nav-item\">\n                        <a class=\"nav-link mbr-medium link text-white text-primary\" href=\"index.html\" data-app-selector=\".nav-link mbr-medium\" data-app-placeholder=\"Type Text\" aria-expanded=\"false\">&nbsp;IBC-Home</a>\n                    </li><li class=\"nav-item dropdown\">\n                        <a class=\"nav-link mbr-medium link dropdown-toggle text-white\" href=\"#\" data-app-selector=\".dropdown-item\" data-toggle=\"dropdown-submenu\" aria-expanded=\"false\" data-app-placeholder=\"Type Text\">Asset Marketplace</a>\n                        <div class=\"dropdown-menu mbr-medium\">\n                            <a class=\"dropdown-item text-white text-primary\" href=\"page31.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\">Going Concerns</a><a class=\"dropdown-item text-white text-primary\" href=\"page30.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\">Standalone Assets</a>\n                            \n                        </div>\n                    </li><li class=\"nav-item dropdown\"><a class=\"nav-link mbr-medium link dropdown-toggle text-white\" href=\"#\" data-app-selector=\".nav-link mbr-medium\" data-app-placeholder=\"Type Text\" data-toggle=\"dropdown-submenu\" aria-expanded=\"false\">Service Marketplace<br></a><div class=\"dropdown-menu mbr-medium\"><a class=\"dropdown-item text-white text-primary\" href=\"page25.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\">Debtors / Guarantors</a><a class=\"dropdown-item text-white text-primary\" href=\"page19.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\">Creditors / Claimants</a><a class=\"dropdown-item text-white text-primary\" href=\"page21.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\">Investors / Applicants</a><a class=\"dropdown-item text-white text-primary\" href=\"page23.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\">Professionals / Lawyers</a></div></li>\n                    \n\n                    <li class=\"nav-item dropdown\">\n                        <a class=\"nav-link mbr-medium link dropdown-toggle text-white\" href=\"#\" data-app-selector=\".nav-link mbr-medium\" data-app-placeholder=\"Type Text\" data-toggle=\"dropdown-submenu\" aria-expanded=\"false\">Insolvency Toolkit</a><div class=\"dropdown-menu mbr-medium\"><a class=\"dropdown-item text-white text-primary\" href=\"page34.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\" aria-expanded=\"false\">Toolkit Modules&nbsp;</a><a class=\"dropdown-item text-white text-primary\" href=\"page35.html\" data-app-selector=\".dropdown-item\" data-app-placeholder=\"Type Text\" aria-expanded=\"false\">Security Features</a></div>\n                    </li></ul>\n                <!-- NAVBAR ITEMS END -->\n                \n            </div>\n            <div class=\"questions mbr-flex mbr-align-center mbr-jc-c\" mbr-if=\"showQuest\">\n                <div class=\"quest__icon mbr-flex mbr-jc-c mbr-align-center\">\n                    <a href=\"http://evoting.resolutionbazaar.com\" target=\"_blank\"><span class=\"mbr-iconfont mbr-iconfont-btn mobi-mbri-login mobi-mbri\" mbr-icon></span></a>\n                </div>\n                <div class=\"mbr-flex mbr-column\">\n                    <p class=\"mbr-fonts-style text mbr-medium mbr-white\" mbr-theme-style=\"display-4\" data-app-selector=\".text\">Have Any Questions?</p>\n                    <a href=\"#\"><p class=\"mbr-fonts-style phone mbr-semibold mbr-white\" mbr-theme-style=\"display-7\" data-app-selector=\".phone\">9814040900</p></a>\n                </div>\n            </div>\n                \n            <!-- COLLAPSED MENU END -->\n            <button class=\"navbar-toggler hide\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-controls=\"navbarNavAltMarkup\" aria-expanded=\"false\" aria-label=\"Toggle navigation\" mbr-if=\"showItems || showSocial\">\n                <div class=\"hamburger\">\n                    <span></span>\n                    <span></span>\n                    <span></span>\n                    <span></span>\n                </div>\n            </button>\n            <button on=\"tap:sidebar.toggle\" class=\"ampstart-btn hamburger\" mbr-if=\"(showItems)\">\n                <span></span>\n                <span></span>\n                <span></span>\n                <span></span>\n            </button>\n        </div>\n            </div>\n    </nav>\n    <!-- AMP plug -->\n    <amp-sidebar id=\"sidebar\" layout=\"nodisplay\" side=\"{{sidebarSide}}\">\n        <div class=\"builder-sidebar\" id=\"builder-sidebar\">\n            <button on=\"tap:sidebar.close\" class=\"close-sidebar\">\n                <span></span>\n                <span></span>\n            </button>\n        </div>\n        \n    </amp-sidebar>\n    <!-- </div> -->\n</section>",

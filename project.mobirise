        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "h3, h4, p, span": {
              "margin": "0"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".item-box": {
              "background": "@bgLightBox",
              "justify-content": "center",
              "width": "100%",
              "overflow": "auto",
              "@media (min-width: 992px)": {
                "max-width": "1000px"
              },
              "@media (max-width: 991px)": {
                "width": "90%",
                "margin": "1rem",
                "padding-top": "3rem"
              }
            },
            ".item-box-wrapper": {
              "background": "@bgLightBox"
            },
            ".close": {
              "opacity": "0.535",
              "&:before, &:after": {
                "background-color": "contrast(@bgLightBox)"
              }
            },
            ".item-wrapper": {
              "cursor": "pointer"
            },
            ".item:focus, span:focus": {
              "outline": "none"
            },
            ".item": {
              ":hover": {
                "cursor": "pointer"
              },
              ".item-box-text, .cur, .item-box-price": {
                "&:hover": {
                  "cursor": "default"
                }
              }
            },
            ".item-wrapper, .item-box, .item-img": {
              "position": "relative"
            },
            ".item-btn, .item-box-btn": {
              "margin": "0"
            },
            ".item-box-title": {
              "color": "#61188f"
            },
            ".item-box-text": {
              "color": "#61188f"
            },
            ".amp-shop__title": {
              "text-align": "center"
            },
            ".item-box-btn": {
              "display": "inline-block"
            },
            ".item-box-img": {
              "align-items": "center",
              "amp-img": {
                "height": "auto"
              }
            },
            ".item-content": {
              "text-align": "center"
            },
            "& when (@customStyles)": {
              "& when (@addPadding)": {
                "& when not (@imgPadding)": {
                  ".item-wrapper": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                },
                "& when (@imgPadding)": {
                  ".item-img": {
                    "padding-bottom": "0"
                  },
                  ".item-content": {
                    "padding": "(@yPadding * 1rem) (@xPadding * 1rem)"
                  }
                }
              },
              "& when (@boxShadow)": {
                ".item-wrapper": {
                  "box-shadow": "0px 0px (@shadowSize * 1px) 0px rgba(red(@shadowColor), green(@shadowColor), blue(@shadowColor), @shadowOpacity)"
                }
              },
              "& when (@useColorCard)": {
                ".item-wrapper": {
                  "position": "relative",
                  "z-index": "0",
                  "&:after": {
                    "content": "\"\"",
                    "display": "block",
                    "position": "absolute",
                    "top": "0",
                    "bottom": "0",
                    "left": "0",
                    "right": "0",
                    "opacity": "@opacityCard",
                    "background": "@colorCard",
                    "transition": "opacity .3s",
                    "pointer-events": "none",
                    "z-index": "-1"
                  }
                }
              },
              "& when (@useBorderRadius)": {
                ".item-wrapper": {
                  "z-index": "12",
                  "border-radius": "(@borderRadius * 1px)",
                  "overflow": "hidden"
                }
              },
              ".item-wrapper": {
                "color": "@fontColor"
              },
              ".btn": {
                "background-color": "@btnColor !important",
                "border-color": "@btnColor !important",
                "color": "contrast(@btnColor, black, white, 30%) !important"
              },
              ".btn:hover": {
                "background-color": "@btnHoverColor !important",
                "border-color": "@btnHoverColor !important",
                "color": "contrast(@btnHoverColor, black, white, 30%) !important"
              }
            },
            "H2": {
              "color": "#000000"
            },
            ".item-box-title1": {
              "color": "#61188f"
            },
            ".cur1": {
              "color": "#61188f"
            },
            ".item-box-text1": {
              "color": "#61188f"
            },
            ".item-box-price1": {
              "color": "#61188f"
            },
            ".item-box-price": {
              "color": "#61188f"
            },
            ".cur": {
              "color": "#61188f"
            }
          },
          "_name": "ampShop6",
          "_customHTML": "<section class=\"amp-shop\" group=\"Shop\" plugins=\"amp-main, amp-lightbox, amp-bind\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" title=\"Title\" name=\"titleSection\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showText\" title=\"Show Text\">\n\n        <header condition=\"customStyle\">Custom Styles</header>\n        <input type=\"checkbox\" name=\"customStyles\" title=\"Custom Style\" checked=\"\">\n\n        <header condition=\"customStyles\">Card Color</header>\n        <input type=\"checkbox\" name=\"useColorCard\" title=\"Use Background Card\" condition=\"customStyles\">\n        <input type=\"color\" name=\"colorCard\" title=\"Color\" value=\"#ffffff\" condition=\"useColorCard &amp;&amp; customStyles\">\n        <input type=\"range\" title=\"Opacity\" name=\"opacityCard\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"useColorCard &amp;&amp; customStyles\">\n        <input type=\"color\" name=\"fontColor\" title=\"Font Color\" value=\"#61188f\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnColor\" title=\"Btn Color\" value=\"#985cbd\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnHoverColor\" title=\"Hover Btn Color\" value=\"#e2d9f4\" condition=\"customStyles\">\n\n        <header condition=\"customStyles\">Paddings</header>\n        <input type=\"checkbox\" name=\"addPadding\" title=\"Add Padding\" condition=\"customStyles\">\n        <input type=\"checkbox\" name=\"imgPadding\" title=\"No Image Padding\" condition=\"customStyles &amp;&amp; addPadding\">\n        <input type=\"range\" title=\"X Padding\" name=\"xPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"1.5\" condition=\"addPadding &amp;&amp; customStyles\">\n        <input type=\"range\" title=\"Y Padding\" name=\"yPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"2\" condition=\"addPadding &amp;&amp; customStyles\">\n\n        <header condition=\"customStyles\">Box Shadow</header>\n        <input type=\"checkbox\" name=\"boxShadow\" title=\"Shadow\" condition=\"customStyles\">\n        <input type=\"range\" name=\"shadowSize\" title=\"Size\" min=\"0\" max=\"100\" step=\"1\" value=\"0\" condition=\"customStyles &amp;&amp; boxShadow\">\n        <input type=\"range\" name=\"shadowOpacity\" title=\"Opacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"customStyles &amp;&amp; boxShadow\">\n        <input type=\"color\" name=\"shadowColor\" title=\"Color\" condition=\"customStyles &amp;&amp; boxShadow\">\n\n        <header condition=\"customStyles\">Border Radius</header>\n        <input type=\"checkbox\" name=\"useBorderRadius\" title=\"Round Border\" condition=\"customStyles\" checked=\"\">\n        <input type=\"range\" name=\"borderRadius\" title=\"Size\" min=\"0\" max=\"50\" step=\"1\" value=\"20\" condition=\"customStyles &amp;&amp; useBorderRadius\">\n        <header>Background</header>\n        <input type=\"color\" name=\"bgLightBox\" title=\"Bg LightBox\" value=\"#efefef\">\n        <fieldset type=\"background\" name=\"bg\" required=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#e2d9f4\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"amp-shop\">\n            <h2 mbr-text=\"\" mbr-theme-style=\"display-5\" class=\"amp-shop__title mbr-pb-3 mbr-fonts-style\" mbr-if=\"titleSection\"><b>Managed Technology Services</b></h2>\n            <div class=\"mbr-row\">\n                <!--1-->\n                <div on=\"tap:showLightbox6-1\" role=\"button\" tabindex=\"0\" class=\"item shop-image mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 mbr-pb-5\">\n                    <!--Ligtbox-->\n                    <amp-lightbox id=\"showLightbox6-1\" layout=\"nodisplay\">\n                        <div class=\"lightbox\">\n                            <div class=\"item-box mbr-row\">\n                                <button type=\"button\" class=\"close\" aria-label=\"Close\" on=\"tap:showLightbox6-1.close\">\n                                     <span aria-hidden=\"true\"></span>\n                                </button>\n                                <div class=\"item-box-img mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-flex mbr-p-4\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/31599336-1-872x680.jpeg\" layout=\"responsive\" width=\"133.2695984703633\" height=\"100\" alt=\"image\" class=\"placeholder-loader\">\n                                        <div placeholder=\"\" class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/31599336-1-872x680.jpeg\" alt=\"\" title=\"\">\n                                    </amp-img>\n                                </div>\n                                <div class=\"item-box-wrapper mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-p-4\">\n\n                                    <div class
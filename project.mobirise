        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "@media (min-width: 768px)": {
              ".ampTabContainer": {
                "display": "flex",
                "flex-wrap": "wrap",
                "padding": "0"
              }
            },
            ".tabContent": {
              "display": "none",
              "order": "1",
              "width": "100%",
              "background-color": "@SliderBg"
            },
            ".tabButton": {
              "@media (max-width: 991px)": {
                "padding": "10px 15px"
              },
              "@media (max-width: 767px)": {
                "padding": "14px 20px",
                "border": "1px solid darken(@SliderBg, 5.8%)",
                "&:not(.last-child)": {
                  "border-bottom": "none"
                }
              },
              "color": "#185886"
            },
            ".tabButton[selected]": {
              "color": "@primaryColor",
              "background-color": "@SliderBg",
              "outline": "none",
              "@media (max-width: 767px)": {
                "background-color": "transparent",
                "&:not(.last-child)": {
                  "border-bottom": "1px solid darken(@SliderBg, 5.8%)"
                }
              }
            },
